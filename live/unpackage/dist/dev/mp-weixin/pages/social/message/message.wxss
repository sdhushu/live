
.chat {
	width: 100%;
	height: 150rpx;
	/* border-bottom: 1px solid #007AFF; */
	-webkit-box-shadow: darkgrey 10px 10px 30px 5px ;
	        box-shadow: darkgrey 10px 10px 30px 5px ;/*边框 */
	text-align: center;
	font-size: 50rpx;
}
.chattext {
	line-height: 150rpx;
}
.bc {
	background-repeat: no-repeat;
	background-size: 100% 100%;
}
.playindex-search {
	width: 80%;
	height: 5%;
	border-radius: 10rpx;
	background-color: #f1f1f1;
	margin: 0 auto;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	margin-top: 20rpx;
}
.search-logo {
	width: 60rpx;
	height: 60rpx;
	background: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-size: 100% 100%;
}
.add-friend {
	width: 100%;
	height: 15%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	margin-top: 5%;
	/* border: 1rpx solid #3F536E; */
}
.add {
	width: 30%;
	height: 70%;
}
.add-first {
	width: 80%;
	height: 100%;
	border-radius: 80%;
	/* background-color: #00007F; */
	border: 5rpx solid #00007F;
	background-image: url(data:image/png;base64,);
}
.addbc {
	background-color: #007AFF;
}
.message-area {
	width: 100%;
	height: 70%;
	margin-top: 5%;
	/* border: 1rpx solid #4CD964; */
}
.message {
	width: 100%;
	height: 20%;
	/* border: 1rpx solid #999999; */
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.logo-img {
	width: 25%;
	height: 80%;
	background-color: #007AFF;
	border-radius: 80%;
}
.message-box {
	width: 70%;
	height: 100%;
	margin-left: 10rpx;
}
.message-nike {
	font-size: 50rpx;
}
.message-content {
	font-size: 40rpx;
	color: #999999;
}
.message-time {
	width: 15%;
	height: 100%;
	color: #e6e6e6;
	font-size: 35rpx;
}

