
.title_tarbar {
	width: 100%;
	height: 15%;
}
.tarbars {
	font-size: 45rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 100%;
	height: 50%;
	background-color: #eaeaea;
}
.line {
	width: 80rpx;
	height: 12rpx;
	background-color: #4CD964;
	border-radius: 20rpx;
	margin-top: 10rpx;
	margin-left: 30rpx;
	-webkit-transition: -webkit-transform 1s;
	transition: -webkit-transform 1s;
	transition: transform 1s;
	transition: transform 1s, -webkit-transform 1s;
}
.input {
	width: 90%;
	height: 50rpx;
	border: 1px solid #f4f4f4;
	margin-top: 20rpx;
	margin-left: 5%;
	border-radius: 20rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.input_icon {
	width: 50rpx;
	height: 50rpx;
	background-image: url(data:image/png;base64,);
	background-size: 100% 100%;
	background-repeat: no-repeat;
}

