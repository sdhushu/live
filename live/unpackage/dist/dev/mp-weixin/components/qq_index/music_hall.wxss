
.bc {
	background-size: 100% 100%;
	background-repeat: no-repeat;
}
.swiper {
	background-color: #3DA7FF;
	border-radius: 20rpx;
}
.navbar {
	width: 100%;
	height: 150rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.nav {
	width: 25%;
	height: 100%;
	text-align: center;
}
.nav_icon {
	width: 90rpx;
	height: 80rpx;
	margin-top: 20rpx;
	margin-left: 20rpx;
}
.nav_text {
	margin-left: -20rpx;
	font-size: 30rpx;
}
.radio_collect {
	width: 100%;
	/* margin-top: 100rpx; */
	height: 300rpx;
}
.radio_wrap {
	width: 100%;
	height: 350rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	white-space: nowrap;
	overflow-x: scroll;
	overflow-y: hidden;
}
.radio_item {
	width: 250rpx;
	height: 250rpx;
	-webkit-box-flex: 0;
	-webkit-flex: none;
	        flex: none;
}
.radio_items {
	width: 230rpx;
	height: 230rpx;
	margin-left: 30rpx;
	margin-top: 20rpx;
	background-color: #1785FF;
	border-radius: 20rpx;
}
.radio_wrap::-webkit-scrollbar {
	width: 0;
	height: 0;
}
.songs {
	width: 100%;
	height: 400rpx;
}
.songs_title {
	width: 100%;
	height: 80rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.song_button {
	width: 150rpx;
	height: 50rpx;
	border-radius: 30rpx;
	margin-left: 10rpx;
	text-align: center;
	font-size: 30rpx;
	background-color: #aaffff;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.song_button_icon {
	width: 30rpx;
	height: 30rpx;
	background-image: url(data:image/png;base64,);
	background-size: 100% 100%;
	background-repeat: no-repeat;
	margin-left: 20rpx;
	margin-top: 8rpx;
}
.songs_title_text {
	font-size: 30rpx;
	margin-left: 30rpx;
	font-weight: bold;
}
.songs_button_play {
	margin-left: 10rpx;
}
.song_list {
	width: 100%;
	height: 400rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	overflow-x: auto;
	overflow-y: hidden;
	white-space: nowrap;
}
.song_list_wrap {
	width: 90%;
	height: 100%;
	-webkit-box-flex: 0;
	-webkit-flex: none;
	        flex: none;
}
.song_list_item {
	width: 100%;
	height: 33%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.list_item_logowrap {
	width: 20%;
	height: 100%;
}
.list_item_textwrap {
	width: 60%;
	height: 100%;
	margin-left: 20rpx;
	margin-top: 25rpx;
}
.list_item_playwrap {
	width: 10%;
	height: 100%;
}
.item_logo {
	width: 120rpx;
	height: 120rpx;
	border-radius: 20rpx;
	margin-left: 30rpx;
	background-color: #007AFF;
}
.item_texts {
	font-size: 35rpx;
}
.item_text {
	font-size: 25rpx;
	margin-top: 10rpx;
	color: #a1a1a1;
}
.item_play {
	width: 60rpx;
	height: 60rpx;
	background-image: url(data:image/png;base64,);
	background-size: 100% 100%;
	background-repeat: no-repeat;
	margin-top: 30rpx;
}
.song_list::-webkit-scrollbar {
	width: 0;
	height: 0;
}
.songs_list {
	width: 100%;
	height: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	overflow-x: scroll;
	overflow-y: hidden;
	white-space: nowrap;
}
.songs_list_item {
	width: 90%;
	height: 250rpx;
	margin-left: 30rpx;
	border-radius: 20rpx;
	background-color: #1785FF;
	-webkit-box-flex: 0;
	-webkit-flex: none;
	        flex: none;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}

